{
	"manifest_version": 2,
	"name": "Teleporter",
	"version": "0.2.12",
	"content_scripts": [
		{
			"matches":[
				"*://gather.town/*",
				"*://app.gather.town/*",
				"*://staging.gather.town/*"
			],
			"js": ["inject.js"],
			"all_frames": true
		}
	],
  "web_accessible_resources": ["teleport.js", "player-ui.js"],
	"browser_action":{
		"default_icon": "icon.png"
	}
}
